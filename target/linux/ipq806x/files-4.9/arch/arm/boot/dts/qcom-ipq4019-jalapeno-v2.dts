#include "qcom-ipq4019-jalapeno.dts"

/ {
	model = "8devices Jalapeno v2";
	
	soc {
		pinctrl@0x01000000 {
			spi_0_pins: spi_0_pinmux {
				pinmux_cs {
					pins = "gpio54", "gpio59";
				};
				
				pinconf_cs {
					pins = "gpio54", "gpio59";
				};
			};
		};
		
		spi_0: spi@78b5000 {
			cs-gpios = <&tlmm 54 0>,
					   <&tlmm 59 0>;
		};
	};
};
